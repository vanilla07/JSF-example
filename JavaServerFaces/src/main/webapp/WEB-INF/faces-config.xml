<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">

	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
		
	   <locale-config>
   	      <default-locale>fr</default-locale>
   	      <supported-locale>en</supported-locale>
   	   </locale-config>
	   <resource-bundle>
		  <base-name>languages.aloxe</base-name>
		  <var>msg</var>
	   </resource-bundle>
		
	</application>
	
	<navigation-rule>
	    <description>Navigation globale : retour Ã  l'accueil depuis toutes les pages</description>
	    <from-view-id>*</from-view-id>
		<navigation-case>
		    <from-outcome>toindex</from-outcome>
		    <to-view-id>/old/index.xhtml</to-view-id>
		    <redirect />
		</navigation-case>
	    <navigation-case>
	        <from-outcome>dispos</from-outcome>
	        <to-view-id>/dispos.xhtml</to-view-id>
	        <redirect/>
	    </navigation-case>
	    <navigation-case>
	        <from-outcome>listing</from-outcome>
	        <to-view-id>/listing.xhtml</to-view-id>
	        <redirect/>
	    </navigation-case>
	    <navigation-case>
	        <from-outcome>resa</from-outcome>
	        <to-view-id>/reservation.xhtml</to-view-id>
	        <redirect/>
	    </navigation-case>
	    <navigation-case>
	        <from-outcome>admin</from-outcome>
	        <to-view-id>/old/admin.xhtml</to-view-id>
	        <redirect/>
	    </navigation-case>
	    <navigation-case>
	        <from-outcome>loca</from-outcome>
	        <to-view-id>/localisation.xhtml</to-view-id>
	        <redirect/>
	    </navigation-case>
	    <navigation-case>
	        <from-outcome>home</from-outcome>
	        <to-view-id>/home.xhtml</to-view-id>
	        <redirect/>
	    </navigation-case>
	</navigation-rule>
	

</faces-config>
